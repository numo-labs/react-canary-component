{
  "name": "react-canary-component",
  "version": "1.0.0",
  "description": "Starter kit for ES6 React components with hot loading, webpack, babel and testing",
  "main": "index.js",
  "dependencies": {
    "starwars": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-istanbul": "^0.6.0",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "expect": "^1.14.0",
    "file-loader": "^0.8.5",
    "jsdom": "^8.0.4",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.1.0",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "semistandard": "^7.0.5",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "scripts": {
    "build": "webpack --progress",
    "dev": "webpack-dev-server --hot --inline",
    "lint": "semistandard",
    "nocov": "npm run lint && mocha index.test.js --compilers js:babel-register",
    "test": "npm run lint && npm run coverage",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha index.test.js --report lcovonly -- -R spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numo-labs/react-canary-component.git"
  },
  "author": "Jimmy Ruts",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/numo-labs/react-canary-component/issues"
  },
  "homepage": "https://github.com/numo-labs/react-canary-component#readme",
  "semistandard": {
    "globals": [
      "describe",
      "it"
    ]
  }
}
